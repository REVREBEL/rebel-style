    body.sd-dark-mode.glass-background-effect:not(.lms-page.blank-layout) > div:not(.modal) .main-wrapper:before {
        background-image: {{glassBgCss}};
    }
    body.sd-dark-mode.glass-background-effect:not(.lms-page.blank-layout) > div:not(.modal) .main-wrapper .info-col-content:before {
        /*background-image: {{glassBgCss}};*/
    }
    body.sd-dark-mode.glass-background-effect:not(.lms-page.blank-layout) > div:not(.modal) .main-wrapper .sd-import-logs-form .row.form-group:before,
    body.sd-dark-mode.glass-background-effect:not(.lms-page.blank-layout) > div:not(.modal) .main-wrapper .sd-import-logs-result .sd-import-logs-result-title:before {
        background-image: {{glassBgCss}};
    }
</style>

<style role="GlassBackground" type="text/css" ng-if="darkMode_enable && OscarThemeForm.glassBackgroundDarkModeImgPath">
    body.sd-dark-mode.glass-background-effect .main-wrapper:before {
        background-image: {{OscarThemeForm.getImageParamDarkMode()}} !important;
    }

    body.sd-dark-mode.glass-background-effect .main-wrapper .info-col-content:before {
        /*background-image: {{OscarThemeForm.getImageParamDarkMode()}} !important;*/
