            ng-class="{'btn-primary':$selectMultiple.activeMatchIndex === $index, 'select-locked':$select.isLocked(this, $index)}" ui-select-sort="$select.selected">
            <span class="match-text" uis-transclude-append=""></span>
            <i
                class="remove-match material-icons"
                ng-hide="$select.disabled"
                ng-click="$selectMultiple.removeChoice($index)">
                    clear
            </i>
        </span>
    </span>
</span>
