            <script type="text/javascript">
              var sidebarAskButtonOnce = false;
              var sidebarAskBeaconOpen = false;

              function sidebarAskDocument() {
                  var ifr = document.querySelector('#beacon-container iframe[data-cy="FrameComponent"]');
                  if (!ifr) return null;
                  return ifr.contentWindow.document;
              }

              function sidebarAskEscHandler(e) {
                  if (e.keyCode === 27) {
                      sidebarAskButton(true);
                      e.stopImmediatePropagation();
                      e.preventDefault();
                  }
              }

              function sidebarAskEscHandlerAttach() {
                  var d = sidebarAskDocument();
                  if (!d) return;
                  d.removeEventListener('keydown', sidebarAskEscHandler, {capture: true});
                  d.removeEventListener('keyup', sidebarAskEscHandler, {capture: true});
                  d.addEventListener('keydown', sidebarAskEscHandler, {capture: true});
                  d.addEventListener('keyup', sidebarAskEscHandler, {capture: true});
              }

                            var sidebarAskSubmitValue = ' \u202F\u200B\u200C\u200D\u202F';

              function sidebarAskSubmitHandler(e) {
                  if (e.target.className.indexOf('c-AskContactForm__form') === -1) return;
                  var d = sidebarAskDocument();
                  if (!d) return;
                  var f = e.target;
                  var ta = f.querySelector('textarea[id]');
                  if (!(f.querySelector('input[name=name]').value != '' && f.querySelector('input[name=subject]').value != '' && ta.value != '')) return;
                  if (ta.value.indexOf(sidebarAskSubmitValue) !== -1) return;
                  ta.value += sidebarAskSubmitValue;
                  e.stopImmediatePropagation();
                  e.preventDefault();
                  e.stopPropagation();
                  setTimeout(function () {
                      if (ta.value.indexOf(sidebarAskSubmitValue) === -1) ta.value += sidebarAskSubmitValue;
                      f.querySelector('.c-AskContactForm__submitButton').click();
                  }, 10);
                  ta.dispatchEvent(new Event('input', {bubbles: true, cancelable: true}));
                  ta.dispatchEvent(new KeyboardEvent('keyup', {bubbles: true, cancelable: true}));
                  return false;
              }
              
              function sidebarAskButton(onlyClose) {
                  Beacon('config', {
                      docsEnabled: false,
                      hideAvatars: true,
                      mode: 'askFirst',
                      enablePreviousMessages: false,
                      display: {position: 'left', showPoweredBy: false}
                  });
                  var info = Beacon('info');
                  if (!info) {
                      Swal.fire({
                          type: 'warning',
                          html: 'We were unable to load support form.<br>Please disable ad blocking extensions for this page and reload the page',
                      });
                      return;
                  }
                  if (info && info.status.isOpened) {
                      sidebarAskBeaconOpen = false;
                      Beacon('close');
                      return;
                  }
                  if (onlyClose) return;
                  if (!sidebarAskButtonOnce) {
                      sidebarAskButtonOnce = true;
                      Beacon('on', 'open', function (e) {
                          console.log('Beacon open');
                          sidebarAskBeaconOpen = true;
                      });
                      Beacon('on', 'close', function (e) {
                          console.log('Beacon close');
                          sidebarAskBeaconOpen = false;
                      });
                      Beacon('on', 'email-sent', function (e) {
                          console.log('Beacon email-sent');
                          setTimeout(sidebarAskEscHandlerAttach, 100);
                          setTimeout(sidebarAskEscHandlerAttach, 500);
                      });
                      Beacon('on', 'open', function () {
                          setTimeout(function () {
                              var d = sidebarAskDocument();
                              if (!d) return;
                              if (!d.querySelector('style[id="styleSd"]')) {
                                  var style = d.createElement('style');
                                  style.id = 'styleSd';
                                  style.type = 'text/css';
                                  style.innerHTML = '.c-MessageInput__input>textarea{height: max(64px, 100vh - 460px) !important;max-height: unset !important;}'
                                      + '.c-HeaderNav__icon-nav{display:none;}'
                                      + '#closeSd{position: absolute;right: 12px;top: 12px;z-index: 9999999;width: 14px;height: 14px;padding: 6px;box-sizing: content-box;cursor: pointer;font-size: 0;}'
                                      + '#closeSd>svg{width:14px;height:14px;}'
                                  ;
                                  d.getElementsByTagName('head')[0].appendChild(style);
                              }
                              try {
                                  if (!d.querySelector('div[id="closeSd"]')) {
                                      var div = d.createElement('div');
                                      let color = !document.querySelector('body.oscar').classList.contains('branding-theme-mrclean') ? "white" : "#555";
                                      div.id = 'closeSd';
                                      div.innerHTML = '<svg fill="' + color + '"xmlns="http://www.w3.org/2000/svg" viewBox="0 0 384 512"><path d="M342.6 150.6c12.5-12.5 12.5-32.8 0-45.3s-32.8-12.5-45.3 0L192 210.7 86.6 105.4c-12.5-12.5-32.8-12.5-45.3 0s-12.5 32.8 0 45.3L146.7 256 41.4 361.4c-12.5 12.5-12.5 32.8 0 45.3s32.8 12.5 45.3 0L192 301.3 297.4 406.6c12.5 12.5 32.8 12.5 45.3 0s12.5-32.8 0-45.3L237.3 256 342.6 150.6z"/></svg>';
                                      div.onclick = function () {
                                          sidebarAskButton(true);
                                      };
                                      d.getElementsByTagName('body')[0].appendChild(div);
                                  }
                              } catch (e) {
                              }
                              sidebarAskEscHandlerAttach();
                                                            d.removeEventListener('submit', sidebarAskSubmitHandler, {capture: true});
                              d.addEventListener('submit', sidebarAskSubmitHandler, {capture: true});
                                                        }, 100);
                      });
                  }
                  Beacon('prefill', {"name":"Gary Stringham"});
                  Beacon('identify', {"email":"gary@revrebel.io","company":"REVREBEL","avatar":"https:\/\/mission-control.revrebel.io\/resize\/resized\/100x100\/uploads\/user\/picture\/freshdesk_7d013ce6bb7b9bf20841643e8c3b1658.png","companyid":85206,"support-status":"priority"});
                  sidebarAskBeaconOpen = true;
                  Beacon('open');
                  Beacon('navigate', '/ask/message');
              }
          </script>
