              </sd-menu-item>
              <sd-menu-item
                click="EntityTimerAdmin.note(timer)">
                <sd-tr uid="NOTES" />
              </sd-menu-item>
            </sd-menu>
          </timer-after-control>
        </sd-timer-counter>
    </sd-entity-timer>
  </ul>
