          <timer-billable ng-if="EntityTimerAdmin.displayRate" class="billable-amount">({{timer.billableAmount | currency:EntityTimerAdmin.currency}})</timer-billable>
          <timer-after-counter ng-if="timer.disabled">
            <button
              tabindex="1"
              title="Invoice"
              class="entity-timer-invoice"
              ng-if="EntityTimerAdmin.invoicing && timer.completed"
              ng-click="EntityTimerAdmin.invoice(timer)">
               <sd-tr uid="INVOICE"></sd-tr>
            </button>
            <button
              tabindex="1"
              title="Delete"
              class="entity-timer-delete"
              ng-if="!EntityTimerAdmin.invoicing && timer.completed"
              ng-click="EntityTimerAdmin.delete(timer)">
            </button>
