  ngf-drop="Files.processUploads()"
  ng-model="Files.uploads"
  ngf-keep="'distinct'"
  ngf-fix-orientation="true"
  ng-class="{
    'no-select': !Files.$canSelect
  }">

  <sdf-include src="/fa/template/browser/toolbar"></sdf-include>
  <sdf-include src="/fa/template/browser/space_quota"></sdf-include>
  <sdf-include src="/fa/template/browser/main"></sdf-include>
</div>
</script>
<script type="text/ng-template" id="/fa/template/browser/main" role="1" assign-to-window="0"><div class="browser-wrap" ng-switch="Files.status">
  <sdf-include src="/fa/template/browser/header"></sdf-include>
  <ul class="browser-list" ng-switch-when="1">
    <li file-entity="alias" class="entity-wrap" uuid="{{Entity.uuid}}" ng-repeat="Entity in Files.aliasedFiles | filter:Files.$aliasSearch | orderBy:'$sort' track by Entity.tid"></li>
    <li file-entity="alias" class="entity-wrap" uuid="{{Entity.uuid}}" ng-repeat="Entity in Files.currentFiles | filter:Files.$aliasSearch | filter:Files.$defaultFolders | orderBy:Files.$sort as $filesOrdered track by Entity.tid"></li>
    <li file-entity="special" class="entity-wrap" uuid="{{Entity.uuid}}" ng-repeat="Entity in Files.specialFiles track by Entity.tid"></li>
    <li file-entity class="entity-wrap" uuid="{{Entity.uuid}}" ng-repeat="Entity in Files.currentFiles | filter:Files.$search | filter:Files.$generalFolders | orderBy:Files.$sort as $filesOrdered track by Entity.tid"></li>
    <li empty-entity class="entity-wrap empty-wrap" ng-if="Files.emptyList || !(Files.currentFiles.length || $filesOrdered.length || Files.aliasedFiles.length || Files.specialFiles.length)" ng-repeat="empty in Files.emptyEntities"></li>
  </ul>
  <div class="browser-error d-flex justify-content-center" ng-switch-when="2">
      <file-icon icon="file-not-exist" size="{w:250}" class="mt-4" ></file-icon>
  </div>
</div>
</script>
