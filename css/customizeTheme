<script type="text/ng-template" id="timerInvoice.html" role="1" assign-to-window="0"><div class="secondary-content form invoice-form" ng-show="actionController.invoiceShow" ng-class="{'secondary-content-wrapper_active':actionController.invoiceShow,'secondary-content-wrapper_inactive':!actionController.invoiceShow}">
    <div class="secondary-content-close">
        <i class="fa-light fa-xmark" ng-click="actionController.showInvoice()"></i>
    </div>

    <div class="row">
        <div class="col-12 pr-4 invoice-form-actions">
            <a class="sd-btn sd-btn-wide-primary-24px ml-0" ng-click="actionController.changeInvoiceType('newInvoice')" ng-class="{'active-invoice':actionController.values.invoice.invoiceType=='newInvoice'}"><i class="fa-light fa-plus-large"></i><span class="ml-1">New Invoice</span></a>

            <a class="sd-btn sd-btn-wide-primary-24px" ng-click="actionController.changeInvoiceType('editInvoice')" ng-class="{'active-invoice':actionController.values.invoice.invoiceType=='editInvoice'}"><i class="fa-light fa-plus-large"></i><span class="ml-1">Existing Invoice</span></a>

            <a class="sd-btn sd-btn-wide-primary-24px" ng-click="actionController.changeInvoiceType('newAccumulating')" ng-class="{'active-invoice':actionController.values.invoice.invoiceType=='newAccumulating'}"><i class="fa-light fa-plus-large"></i><span class="ml-1">Accumulating Invoice Generator</span></a>

            <a class="sd-btn sd-btn-wide-primary-24px" ng-click="actionController.changeInvoiceType('editAccumulating')" ng-class="{'active-invoice':actionController.values.invoice.invoiceType=='editAccumulating'}"><i class="fa-light fa-plus-large"></i><span class="ml-1">Existing Accumulating Invoice Generator</span></a>
        </div>
    </div>

    <div class="row mt-2">
        <div class="col-12" ng-if="actionController.invoice == 'editInvoice'">
