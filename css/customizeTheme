      ng-repeat="timer in EntityTimerAdmin.timers | orderBy:'-id'">
        <sd-timer-counter
          timer="timer"
          timer-id="timer.id"
          entity-id="timer.entityID"
          show-billable="EntityTimerAdmin.displayRate"
          show-control="!timer.disabled">
