<script type="text/ng-template" id="notification-panel-default-tpl.html">
    <div class="sidebar-block-wrapper" ng-class="{'unread-notification' : notification.read == 0 }">
    <div class="unread-notification-circle">
        <div ng-click="changeReadValue('1'); $event.stopPropagation();"  ></div>
    </div>
    <div class="flex-1 position-relative" ng-click="navigateTo(); $event.stopPropagation();" >
        <div class="d-flex">
            <div class="notification-icon-wrapper">
                <div class="notification-icon" ng-if="notification.userAvatar" ng-bind-html="notification.userAvatar | trustSvg"></div>
                <div class="notification-icon" ng-if="!notification.userAvatar">
                    {{notification.senderDisplayName.charAt(0)}}
                </div>
                <div class="icon">
                    <i ng-class="notification.iconClass"></i>
                </div>

            </div>


            <div class="notification-info">
                <div class="notification-subject flex-1" >
                    {{notification.senderDisplayName}}
                </div>
                <div class="notification-mes-subject">{{notification.subject}}</div>
                <div class="notification-text">{{notification.text}}</div>
            </div>
        </div>
    </div>

    <div class="notification-options">

        <div class="icon-options">
            <div class="icon">
                <div class="dropdown">
    <a class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
        <i class="fa-solid fa-ellipsis-vertical"></i>
    </a>
    <div class="dropdown-menu dropdown-left dropdown-menu-with-icons">
        <a class="dropdown-item" href="#" role="button" ng-click="toggleReadValue(); $event.stopPropagation();">
            <span ng-show="notification.read == 1" >
                <svg><use href="/cachedStatic/iconUrl1?file=%2Fthemes%2Fmpa%2Fassets%2Fsvg%2Fcommon-svg%2Ficons-sprite.svg&hash=XnWtUWQr67Nizt9onlcZ#light-envelope-circle-exclamation"></use></svg>Mark as Unread            </span>
            <span ng-show="notification.read == 0">
                <svg><use href="/cachedStatic/iconUrl1?file=%2Fthemes%2Fmpa%2Fassets%2Fsvg%2Fcommon-svg%2Ficons-sprite.svg&hash=XnWtUWQr67Nizt9onlcZ#envelope-circle-check"></use></svg>Mark as Read            </span>
        </a>
        <a class="dropdown-item" href="#" role="button" ng-click="toggleStickyValue(); $event.stopPropagation();">
            <span ng-show="notification.sticky == 1 && notification.showUN == 0" >
                <svg><use href="/cachedStatic/iconUrl1?file=%2Fthemes%2Fmpa%2Fassets%2Fsvg%2Fcommon-svg%2Ficons-sprite.svg&hash=XnWtUWQr67Nizt9onlcZ#thumbtack-slash"></use></svg>Unpin            </span>
            <span ng-show="notification.sticky == 0">
                <svg><use href="/cachedStatic/iconUrl1?file=%2Fthemes%2Fmpa%2Fassets%2Fsvg%2Fcommon-svg%2Ficons-sprite.svg&hash=XnWtUWQr67Nizt9onlcZ#thumbtack"></use></svg>Pin            </span>
        </a>

    </div>
</div>            </div>
        </div>
        <div class="received-time">
            {{dateDiff(notification.date)}}
        </div>
    </div>

</div></script>

