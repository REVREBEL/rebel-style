<script type="text/ng-template" id="fileUploader.html" role="1" assign-to-window="0"><div class="file-upload-widget sd-oscar-file-upload-wrapper">
    <div class="mr-b-10" ng-if="OscarFileUpload.showPreview && ((OscarFileUpload.isImage && OscarFileUpload.file) || ${tAttrs.modelAttribute})">
        <div class="image-preview" ng-show="${tAttrs.modelAttribute} || OscarFileUpload.file">
            <img ngf-thumbnail="OscarFileUpload.preview" ng-if="OscarFileUpload.file && OscarFileUpload.cropEnabled">
            <img ng-src="${tAttrs.path}{{${tAttrs.modelAttribute}}}" ng-if="${tAttrs.modelAttribute} && (OscarFileUpload.showCropPreview || !OscarFileUpload.file)" />
        </div>
        <div class="crop-area" ng-show="OscarFileUpload.cropEnabled && OscarFileUpload.isImage && OscarFileUpload.cropImage">
            <div class="row">
                <div ng-class="{'col-6': OscarFileUpload.cropView == 'horizontal', 'col-12': OscarFileUpload.cropView == 'vertical'}" class="crop-block">
                    <img-crop
                        area-type="${tAttrs.areaType}"
                        aspect-ratio="${tAttrs.aspectRatio}"
                        image="OscarFileUpload.cropImage"
                        result-image="OscarFileUpload.croppedImage"
                        result-image-size="${tAttrs.resultImageSize}"
                        init-max-area="true">
                    </img-crop>
                </div>
                <div class="d-flex justify-content-center align-items-center" ng-class="{'col-6': OscarFileUpload.cropView == 'horizontal', 'col-12': OscarFileUpload.cropView == 'vertical'}">
                    <img ngf-thumbnail="OscarFileUpload.croppedImage">
                </div>
            </div>
        </div>
    </div>
    <div class="mr-b-10">
        <div
            class="drop-box"
            ng-model="OscarFileUpload.file"
            ngf-multiple="false"
            ngf-pattern="'${filePattern}'"
            ngf-drag-over-class="'dragover'"
            ngf-fix-orientation="true"
            ngf-drop="OscarFileUpload.prepareFile(OscarFileUpload.file)"
            ngf-select="OscarFileUpload.prepareFile(OscarFileUpload.file)"
            ng-if="!${tAttrs.modelAttribute} && !OscarFileUpload.file && !OscarFileUpload.showKeepBtn">Select or Drop Files</div>
        <div ngf-no-file-drop>File Drag/Drop is not supported for this browser</div>
    </div>
    <div class="mr-b-10" ng-if="!OscarFileUpload.isImage">
        <span ng-if="OscarFileUpload.file">{{OscarFileUpload.file.name}}</span>
    </div>
    <div class="btn-list">
        <button
            type="button"
            ng-if="!${tAttrs.modelAttribute} && !OscarFileUpload.file && !OscarFileUpload.showKeepBtn"
            ng-model="OscarFileUpload.file"
            class="btn btn-primary btn-sm"
            ngf-pattern="'${filePattern}'"
            ngf-fix-orientation="true"
            ngf-select="OscarFileUpload.prepareFile(OscarFileUpload.file)">
            {{OscarFileUpload.buttonText}}
        </button>
        <button
            type="button"
            ng-if="${tAttrs.modelAttribute} && !OscarFileUpload.showKeepBtn"
            ng-model="OscarFileUpload.file"
            class="btn btn-primary btn-sm"
            ngf-pattern="'${filePattern}'"
            ngf-fix-orientation="true"
            ngf-select="OscarFileUpload.replaceFile(OscarFileUpload.file)">
            {{OscarFileUpload.buttonReplaceText}}
        </button>
        <button
            type="button"
            ng-if="OscarFileUpload.showKeepBtn && OscarFileUpload.cropEnabled"
            class="btn btn-primary btn-sm"
            ng-click="OscarFileUpload.keepPreppedFile()">
            {{OscarFileUpload.buttonAcceptCropText}}
        </button>
        <button
            type="button"
            ng-if="OscarFileUpload.showKeepBtn || OscarFileUpload.uploadErrorMessage"
            class="btn btn-danger btn-sm"
            ng-click="OscarFileUpload.cancel()">
            {{OscarFileUpload.buttonCancelText}}
        </button>
        <button
            type="button"
            ng-if="${tAttrs.modelAttribute} && !OscarFileUpload.showKeepBtn"
            class="btn btn-primary btn-sm"
            ng-click="OscarFileUpload.deleteFile()">
            {{OscarFileUpload.buttonDeleteText}}
        </button>
    </div>
    <div class="text-muted">${tAttrs.info || ''}</div>
</div></script>
<script type="text/ng-template" id="multiFileUploader.html" role="1" assign-to-window="0"><div class="file-upload-widget sd-oscar-file-upload-wrapper multiple-files">
    <div>
        <div class="mr-b-10" ng-if="config.dropZone">
            <div
            class="drop-box w-100"
            ngf-drop="Uploader.uploadSelected($newFiles, $invalidFiles)"
            ngf-multiple="true"
            ngf-keep="'distinct'"
            ngf-fix-orientation="true"
            ngf-drag-over-class="'dragover'"
            ng-class="{'has-files':files.length}">
                <p
                    ngf-select="Uploader.uploadSelected($newFiles, $invalidFiles)"
                    ngf-multiple="true"
                    ngf-allow-dir="true"
                    ngf-keep="'distinct'"
                    ngf-fix-orientation="true"
                    ng-if="!files.length"><sd-tr uid="SELECT_OR_DROP_FILES" /></p>
                <div ng-if="files.length" class="file-list">
                    <ul>
                        <li class="file" ng-repeat="file in files">
                            <div class="info">
                                <span class="fileName">{{file.fileName}}</span>

                                <uib-progressbar
                                class="progress-lg"
                                value="file.progress || 0"
                                type="{{file.cancelled ? 'danger' : (file.uploaded ? 'success' : 'info')}}">
                                    <span ng-if="!file.cancelled">{{file.progress ? (file.progress < 100 ? file.progress + '%' : 'Done') : '0%'}}</span>
                                    <span ng-if="file.cancelled">{{file.aborted ? 'Cancelled' : 'Error'}}</span>
                                </uib-progressbar>

                                <button ng-if="!file.aborted && !file.uploaded" class="btn btn-xs btn-danger" ng-click="file.abort()">
                                    <i class="material-icons list-icon">clear</i>
                                </button>
                            </div>
                            <div class="error" ng-if="file.error">{{file.errorMessage}}</div>
                        </li>
                    </ul>
                </div>
            </div>
            <div ngf-no-file-drop><sd-tr uid="FILE_DRAG_DROP_NOT_SUPPORTED" /></div>
        </div>
        <div class="btn-list">
            <button
            type="button"
            class="btn btn-primary btn-sm"
            ngf-select="Uploader.uploadSelected($newFiles, $invalidFiles)"
            ngf-multiple="true"
            ngf-fix-orientation="true"
            ngf-keep="'distinct'"
            ng-class="{'btn-block':!files.length}">
                <sd-tr uid="SELECT_FILES" />
            </button>
            <div class="d-inline-block" ng-if="files.length">
                <button
                ng-if="!config.autoUpload"
                type="button"
                class="btn btn-success btn-sm"
                ng-disabled="OscarFileUpload.uploadInProgress || !OscarFileUpload.files.length"
                ng-click="OscarFileUpload.uploadAll()">
                    <ng-pluralize
                    ng-hide="OscarFileUpload.uploadInProgress"
                    count="OscarFileUpload.files.length"
                    when="{
                        'one' : 'Upload File',
                        'other' : 'Upload {} Files'
                    }">
                    </ng-pluralize>
                    <span ng-show="OscarFileUpload.uploadInProgress"><sd-tr uid="UPLOADING_ELLIPSES" /></span>
                </button>
                <button
                    type="button"
                    class="btn btn-danger btn-sm"
                    ng-click="OscarFileUpload.clearAll()">
                  <sd-tr uid="REMOVE_CANCEL_ALL" />
                </button>
            </div>
        </div>
    </div>
</div>
</script>
<script type="text/ng-template" id="filesList.html" role="1" assign-to-window="0"><div>
    <ul class="file-list list-unstyled" ng-show="files.length">
        <li ng-repeat="file in files | orderBy:'+created' track by file.id" class="mb-1 d-flex justify-content-between">
            <a href="#" ng-click="download(file)" class="name m">
                <span>{{file.title || file.name}}</span>
            </a>
            <div class="d-flex ml-auto btns-wrap">
                <button
                    type="button"
                    class="btn btn-secondary btn-xs"
                    ng-click="download(file)"
                    g-show="config.canDownload">
                    <svg class="inCourse-file-icon icon-download"><use href="/cachedStatic/iconUrl1?file=%2Fthemes%2Fmpa%2Fassets%2Fsvg%2Fcommon-svg%2Ficons-sprite.svg&hash=XnWtUWQr67Nizt9onlcZ#download"></use></svg>                </button>
                <button
                    type="button"
                    class="btn btn-secondary  btn-xs"
                    ng-click="preview(file)"
                    ng-show="canPreview(file)">
                    <svg class="inCourse-file-icon"><use href="/cachedStatic/iconUrl1?file=%2Fthemes%2Fmpa%2Fassets%2Fsvg%2Fcommon-svg%2Ficons-sprite.svg&hash=XnWtUWQr67Nizt9onlcZ#eye"></use></svg>                </button>
                <button
                    class="btn btn-xs btn-secondary sd-file-task-upload-btn-delete"
                    sd-modal-confirm
                    ng-if="config.canDelete"
                    action-url="/file/delete?id={{file.id}}"
                    success="ctrl.removeFile(file)">
                    <svg class="inCourse-file-icon"><use href="/cachedStatic/iconUrl1?file=%2Fthemes%2Fmpa%2Fassets%2Fsvg%2Fcommon-svg%2Ficons-sprite.svg&hash=XnWtUWQr67Nizt9onlcZ#trash-can"></use></svg>                </button>
                <!--<button class="btn btn-xs btn-default" ng-click="$fileInfo = !$fileInfo">Toggle Info</button>-->
                <!--<pre ng-show="$fileInfo">{{file | json:2}}</pre>-->
            </div>
        </li>
    </ul>
    <div class="no-files-message py-3" ng-hide="files.length">
        <sd-tr uid="NO_FILES_ADDED" />
    </div>
</div></script>
<script type="text/ng-template" id="fileUploaderV2.html" role="1" assign-to-window="0"><div class="file-upload-widget sd-oscar-file-upload-wrapper multiple-files">
    <div>
        <div class="mr-b-10" ng-if="uploadReady">
            <div
                    class="drop-box w-100"
                    ngf-drop="UploaderV2.uploadSelected($newFiles, $invalidFiles)"
                    ngf-multiple="false"
                    ngf-allow-dir="false"
                    ngf-keep="false"
                    ngf-fix-orientation="true"
                    ngf-drag-over-class="'dragover'"
                            >
                <p
                        ngf-select="UploaderV2.uploadSelected($newFiles, $invalidFiles)"
                        ngf-multiple="false"
                        ngf-allow-dir="false"
                        ngf-keep="false"
                        ngf-fix-orientation="true"
                        style="margin:0; padding:0; flex-grow:1; line-height:196px;"
                >
