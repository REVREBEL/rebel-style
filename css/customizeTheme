<script type="text/ng-template" id="onboardingWizardPlatformBrandingTemplate.html" role="1" assign-to-window="0"><form class="form-oscar platformBranding" name="platformBranding" id="organization-form" sd-oscar-form="organizationform" action="/company/customizeTheme" method="post">    <h3 class="form-title">Preset Themes</h3>
    <div class="form-group">
        <select ng-model="$root.themeName" ng-change="changePresetTheme($root.themeName)" class="form-control" sd-oscar-theme-select="select2" data-options="{&#039;allowClear&#039;:false}" name="Company[themeName]" id="Company_themeName">
